			<div id="contact">
				<div id="phone">
					<img id="telephone" src="./image/telephone.png" alt="">
					<p>06.47.30.53.80</p>
				</div>
				<div id="email">
					<img id="mail" src="./image/Mail.png" alt="">
					<p>nathanweber1706@gmail.com</p>
				</div>
			</div>
